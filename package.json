{
  "name": "victor-hugo",
  "version": "1.0.0",
  "description": "Victor Hugo is a Hugo boilerplate for creating truly epic websites!",
  "repository": "netlify/victor-hugo",
  "main": "index.js",
  "scripts": {
    "lint": "eslint src",
    "start": "run-p start:**",
    "start:hugo": "hugo -d dist -vw",
    "start:webpack": "webpack-dev-server --config webpack.dev.js",
    "preview": "run-p preview:**",
    "preview:hugo": "npm run start:hugo -- -D -F",
    "preview:webpack": "npm run start:webpack",
    "prebuild": "rimraf dist",
    "build": "npm run build:webpack && npm run build:hugo",
    "build:preview": "npm run build:webpack && npm run build:hugo:preview",
    "build:hugo": "hugo -d dist -v --minify --baseURL=https://jesserieser.com",
    "build:hugo:preview": "npm run build:hugo -- -D -F ",
    "build:webpack": "cross-env NODE_ENV=production webpack --config webpack.prod.js --hot --inline",
    "build:forestry": "npm run build:forestry:webpack && npm run build:forestry:preview",
    "build:forestry:webpack": "cross-env NODE_ENV=production webpack --config webpack.forestry.js --hot --inline",
    "build:forestry:preview": "hugo server -D -E -F --port 8080 --bind 0.0.0.0 --renderToDisk -d dist -v -w"
  },
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.9.5",
    "@babel/plugin-syntax-object-rest-spread": "^7.8.3",
    "@babel/preset-env": "^7.9.5",
    "@babel/register": "^7.9.0",
    "@fullhuman/postcss-purgecss": "^2.1.2",
    "assets-webpack-plugin": "^3.9.12",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.1.0",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^5.1.1",
    "cross-env": "^7.0.2",
    "css-loader": "^3.5.2",
    "eslint": "^6.8.0",
    "eslint-plugin-import": "^2.20.2",
    "exports-loader": "^0.7.0",
    "fancy-log": "^1.3.3",
    "file-loader": "^6.0.0",
    "hugo-bin": "^0.57.0",
    "imports-loader": "^0.8.0",
    "mini-css-extract-plugin": "^0.9.0",
    "node-sass": "^4.13.1",
    "npm-run-all": "^4.1.5",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "plugin-error": "^1.0.1",
    "postcss-import": "^12.0.1",
    "postcss-loader": "^3.0.0",
    "postcss-preset-env": "^6.7.0",
    "rimraf": "^3.0.2",
    "sass-loader": "^8.0.2",
    "style-loader": "^1.1.4",
    "tailwindcss": "^1.5.1",
    "tailwindcss-aspect-ratio": "^3.0.0",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "url-loader": "^4.1.0",
    "webpack": "^4.42.1",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3",
    "webpack-merge": "^4.2.2",
    "whatwg-fetch": "^3.0.0"
  },
  "resolutions": {
    "natives": "1.1.6"
  },
  "dependencies": {
    "@swup/body-class-plugin": "^1.0.2",
    "@swup/fade-theme": "^1.0.5",
    "@swup/ga-plugin": "^1.0.1",
    "@swup/gtm-plugin": "^1.0.2",
    "@swup/preload-plugin": "^1.0.3",
    "@swup/scripts-plugin": "^1.1.0",
    "@swup/scroll-plugin": "^1.0.6",
    "browser-sync": "^2.26.7",
    "flickity": "^2.2.1",
    "gulp": "^4.0.2",
    "imagesloaded": "^4.1.4",
    "isotope-layout": "^3.0.6",
    "lazysizes": "^5.2.0",
    "midday.js": "^1.0.2",
    "photoswipe": "^4.1.3",
    "postcss-nested": "^4.2.1",
    "rellax": "^1.12.1",
    "sal.js": "^0.7.4",
    "scroll-lock": "^2.1.4",
    "smoothscroll-polyfill": "^0.4.4",
    "swup": "^2.0.9",
    "swup-gtag-plugin": "0.0.0",
    "tailwindcss-flexbox-order": "^1.1.0"
  }
}
